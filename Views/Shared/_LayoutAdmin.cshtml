    <!-- Custom CSS for Colors and Enhanced Hover Animations -->
    <style>
        :root {
            --primary: #1976D2;
            --accent: #FFFFFF;
            --accent-hover: #E3F2FD;
            --background: #FAFAFA;
            --text: #212121;
            --text-muted: #757575;
        }

        /* General */
        body {
            background: var(--background);
            color: var(--text);
        }

        /* Header */
        .header {
            background: var(--primary);
            color: var(--accent);
        }

        .logo span {
            color: var(--accent);
            transition: opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

        .logo:hover span {
            opacity: 0.9;
            transform: scale(1.05);
        }

        .toggle-sidebar-btn {
            color: var(--accent);
            transition: color 0.3s ease-in-out, opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

            .toggle-sidebar-btn:hover {
                color: var(--accent-hover);
                opacity: 0.9;
                transform: scale(1.05);
            }

        .search-form input {
            background: rgba(255, 255, 255, 0.15);
            color: var(--accent);
            border: none;
            transition: background 0.3s ease-in-out;
        }

            .search-form input::placeholder {
                color: rgba(255, 255, 255, 0.7);
            }

            .search-form input:focus {
                background: rgba(255, 255, 255, 0.25);
            }

        .search-form button {
            color: var(--accent);
            transition: color 0.3s ease-in-out, opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

            .search-form button:hover {
                color: var(--accent-hover);
                opacity: 0.9;
                transform: scale(1.05);
            }

        .header-nav .nav-icon {
            color: var(--accent);
            transition: color 0.3s ease-in-out, opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

            .header-nav .nav-icon:hover {
                color: var(--accent-hover);
                opacity: 0.9;
                transform: scale(1.05);
            }

        .nav-profile span {
            color: var(--accent);
            transition: opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

        .nav-profile:hover span {
            opacity: 0.9;
            transform: scale(1.05);
        }

        .dropdown-menu .dropdown-item {
            color: var(--text);
            transition: background 0.3s ease-in-out, color 0.3s ease-in-out, opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

            .dropdown-menu .dropdown-item:hover {
                background: var(--accent-hover);
                color: var(--primary);
                opacity: 0.9;
                transform: scale(1.05);
            }

        /* Sidebar */
        .sidebar {
            background: var(--background);
        }

        .sidebar-nav .nav-link {
            color: var(--text);
            background: var(--accent);
            transition: background 0.3s ease-in-out, color 0.3s ease-in-out, opacity 0.3s ease-in-out, transform 0.3s ease-in-out, box-shadow 0.3s ease-in-out;
        }

            .sidebar-nav .nav-link i {
                color: var(--text-muted);
                margin-right: 10px;
                font-size: 1.2rem;
                transition: color 0.3s ease-in-out, transform 0.3s ease-in-out;
            }

            .sidebar-nav .nav-link:hover,
            .sidebar-nav .nav-link.collapsed:not(.nav-heading) {
                background: var(--accent-hover);
                color: var(--primary);
                opacity: 0.9;
                transform: scale(1.05);
                box-shadow: 0 2px 4px rgba(25, 118, 210, 0.2);
            }

                .sidebar-nav .nav-link:hover i,
                .sidebar-nav .nav-link.collapsed:not(.nav-heading) i {
                    color: var(--primary);
                    transform: scale(1.05);
                }

        .sidebar-nav .nav-heading {
            color: var(--text-muted);
        }

        /* Footer */
        .footer {
            background: var(--primary);
            color: var(--accent);
        }

            .footer a {
                color: var(--accent);
                transition: opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
            }

                .footer a:hover {
                    opacity: 0.9;
                    transform: scale(1.05);
                }

        /* Back to Top */
        .back-to-top {
            background: var(--primary);
            color: var(--accent);
            transition: background 0.3s ease-in-out, opacity 0.3s ease-in-out, transform 0.3s ease-in-out;
        }

            .back-to-top:hover {
                background: var(--accent-hover);
                opacity: 0.9;
                transform: scale(1.05);
            }

        /* Scrollbar Styling */
        ::-webkit-scrollbar {
            width: 8px;
        }

        ::-webkit-scrollbar-track {
            background: var(--background);
        }

        ::-webkit-scrollbar-thumb {
            background: var(--primary);
            border-radius: 4px;
        }
    </style>
</head>

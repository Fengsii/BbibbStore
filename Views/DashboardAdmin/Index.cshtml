<style>
    :root {
        --primary: #4154f1;
        --primary-light: #f6f6fe;
        --secondary: #717ff5;
        --success: #2eca6a;
        --warning: #ffc107;
        --danger: #ff4d4f;
        --dark: #212529;
        --light: #f8f9fa;
        --text: #444444;
        --text-muted: #6c757d;
        --border: #e9ecef;
        --shadow: 0 2px 20px rgba(0, 0, 0, 0.08);
    }

    .main {
        background-color: #f6f9ff;
        padding: 20px;
    }

    .pagetitle {
        margin-bottom: 30px;
    }

        .pagetitle h1 {
            font-size: 2rem;
            font-weight: 600;
            color: var(--dark);
            margin-bottom: 0.5rem;
        }

    .breadcrumb {
        background-color: transparent;
        padding: 0;
    }

    .breadcrumb-item a {
        color: var(--text-muted);
        text-decoration: none;
    }

    .breadcrumb-item.active {
        color: var(--primary);
    }

    .card {
        border: none;
        border-radius: 10px;
        box-shadow: var(--shadow);
        margin-bottom: 24px;
        transition: transform 0.3s ease, box-shadow 0.3s ease;
    }

        .card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 25px rgba(0, 0, 0, 0.1);
        }

    .card-body {
        padding: 1.5rem;
    }

    .card-title {
        color: var(--dark);
        font-weight: 600;
        margin-bottom: 1.5rem;
    }

        .card-title span {
            color: var(--text-muted);
            font-size: 0.9rem;
            font-weight: 400;
        }

    .info-card {
        border-left: 4px solid var(--primary);
    }

        .info-card .card-icon {
            font-size: 1.5rem;
            color: var(--primary);
            background-color: var(--primary-light);
            width: 50px;
            height: 50px;
        }

        .info-card h6 {
            font-size: 1.5rem;
            font-weight: 700;
            color: var(--dark);
            margin-bottom: 0.25rem;
        }

    .recent-sales {
        background-color: white;
    }

        .recent-sales table {
            color: var(--text);
        }

        .recent-sales th {
            font-weight: 600;
            color: var(--dark);
        }

    .badge {
        font-weight: 500;
        padding: 0.35em 0.65em;
    }

    .top-selling .activity {
        padding-left: 1rem;
    }

    .activity-item {
        position: relative;
        padding-bottom: 1.5rem;
        border-left: 1px solid var(--border);
    }

        .activity-item:last-child {
            border-left: 0;
            padding-bottom: 0;
        }

        .activity-item::before {
            content: '';
            position: absolute;
            width: 12px;
            height: 12px;
            border-radius: 50%;
            left: -6px;
            top: 0;
            background: var(--primary);
        }

    .activite-label {
        color: var(--text-muted);
        font-size: 0.75rem;
        min-width: 60px;
    }

    .activity-content {
        padding-left: 1rem;
    }

        .activity-content a {
            font-weight: 600;
            color: var(--dark);
            text-decoration: none;
        }

        .activity-content span {
            display: block;
            color: var(--text-muted);
            font-size: 0.85rem;
        }

    .stats-grid {
        display: grid;
        grid-template-columns: 1fr 1fr;
        gap: 1rem;
    }

    .stat-item {
        display: flex;
        align-items: center;
        padding: 0.75rem;
        background-color: var(--light);
        border-radius: 8px;
    }

        .stat-item i {
            font-size: 1.25rem;
            color: var(--primary);
            margin-right: 0.75rem;
        }

        .stat-item span {
            display: block;
            font-size: 0.75rem;
            color: var(--text-muted);
        }

        .stat-item h6 {
            font-size: 1rem;
            font-weight: 600;
            margin-bottom: 0;
            color: var(--dark);
        }

    @@media (max-width: 992px) {
        .stats-grid

    {
        grid-template-columns: 1fr;
    }

    }
</style>
